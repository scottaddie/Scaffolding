{
  "version": "1.2.0-*",
  "buildOptions": {
    "keyFile": "../../tools/Key.snk",
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "description": "Contains the CodeGenCommand that finds the appropriate code generator and invokes it from project dependencies.",
  "dependencies": {
    "Microsoft.Extensions.CommandLineUtils": "1.2.0-*",
    "Microsoft.Extensions.DependencyInjection": "1.2.0-*",
    "Microsoft.VisualStudio.Web.CodeGeneration.EntityFrameworkCore": {
      "target": "project"
    },
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.6": {
      "imports": [
        "portable-net45+win8"
      ]
    }
  },
  "packOptions": {
    "tags": [
      "aspnetcore",
      "codegenerator",
      "scaffolding",
      "visualstudioweb"
    ]
  }
}